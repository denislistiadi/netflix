,"fixableErrorCount":0,"fixableWarningCount":0},{"filePath":"184","messages":"185","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"98"},"/media/denis/New Volume/netflix/src/components/card/index.js",[],["186","187"],"/media/denis/New Volume/netflix/src/components/card/styles/card.js",[],"/media/denis/New Volume/netflix/src/containers/browse.js",["188"],"import React, { useContext, useEffect, useState } from 'react';\nimport Fuse from 'fuse.js';\nimport { SelectProfileContainer } from './profiles';\nimport { FirebaseContext } from '../context/firebase';\nimport { Header, Loading, Card, Player } from '../components';\nimport * as ROUTES from '../constants/routes';\nimport logo from '../logo.svg';\nimport {FooterContainer} from './footer';\nimport { Item } from '../components/jumbotron/styles/jumbotron';\n\nexport function BrowseContainer({slides}) {\n    const [category, setCategory] = useState('series');\n    const [slideRows, setSlideRows] = useState([]);\n\n    const [searchTerm, setSearchTerm] = useState('');\n    const [profile, setProfile] = useState({});\n    const [loading,setLoading] = useState(true);\n\n    const { firebase } = useContext(FirebaseContext);\n    const user = firebase.auth().currentUser || {};\n\n    useEffect(() => {\n        setTimeout(() => {\n            setLoading(false);\n        }, 3000);\n    }, [profile.displayName]);\n\n    useEffect(() => {\n        setSlideRows(slides[category]);\n    }, [slides, category]);\n\n    useEffect(() => {\n        const fuse = new Fuse(\n            slideRows, { keys: ['data.description', 'data.title', 'data.genre'],}\n        );\n        const results = fuse.search(searchTerm).map(({ item }) => item );\n\n        if (slideRows.length > 0 && searchTerm.length > 3 && results.length > 0) {\n            setSlideRows(results);\n        } else {\n            setSlideRows(slides[category]);\n        }\n    }, [searchTerm])\n\n    return profile.displayName ? (\n        <>\n            {loading ? <Loading src={user.photoURL} /> : <Loading.ReleaseBody />}\n            <Header src='joker1' dontShowOnSmallViewPort >\n                <Header.Frame>\n                    <Header.Group>\n                        <Header.Logo to={ROUTES.HOME} src={logo} alt=\"Netflix\" />\n                        <Header.TextLink \n                            active={category === 'series' ? 'true' : 'false'}\n                            onClick={() => setCategory('series')}>\n                                Series\n                        </Header.TextLink>\n                        <Header.TextLink \n                            active={category === 'films' ? 'true' : 'false'}\n                            onClick={() => setCategory('films')}>\n                                Films\n                        </Header.TextLink>\n                    </Header.Group>\n\n                    <Header.Group>\n                        <Header.Search \n                            searchTerm={searchTerm} \n                            setSearchTerm={setSearchTerm} \n                        />\n                        <Header.Profile>\n                            <Header.Picture src={user.photoURL} />\n                            <Header.Dropdown>\n                                <Header.Group>\n                                    <Header.Picture src={user.photoURL} />\n                                    <Header.TextLink>{user.displayName}</Header.TextLink>\n                                </Header.Group>\n                                <Header.Group>\n                                    <Header.TextLink onClick={() => firebase.auth().signOut()}>\n                                        Sign Out\n                                    </Header.TextLink>\n                                </Header.Group>\n                            </Header.Dropdown>\n                        </Header.Profile>\n                    </Header.Group>\n                </Header.Frame>\n\n                <Header.Feature>\n                    <Header.FeatureCallOut>\n                        Watch Joker Now\n                    </Header.FeatureCallOut>\n                    <Header.Text>\n                        Forever alone in a crowd, failed comedian Arthur Fleck seeks connection as he walks the streets of Gotham\n                        City. Arthur wears two masks -- the one he paints for his day job as a clown, and the guise he projects in a\n                        futile attempt to feel like he's part of the world around him.\n                    </Header.Text>\n                    <Header.PlayButton>Play</Header.PlayButton>\n                </Header.Feature>\n            </Header>\n\n            <Card.Group>\n                {slideRows.map((slideItem) => (\n                    <Card key={`${category}-${slideItem.title.toLowerCase()}`}>\n                        <Card.Title>{slideItem.title}</Card.Title>\n                        <Card.Entities>\n                            {slideItem.data.map((item) => (\n                                <Card.Item key={Item.docId} item={item}>\n                                    <Card.Image src={`/images/${category}/${item.genre}/${item.slug}/small.jpg`} />\n                                    <Card.Meta>\n                                        <Card.SubTitle>{item.title}</Card.SubTitle>\n                                        <Card.Text>{item.description}</Card.Text>\n                                    </Card.Meta>\n                                </Card.Item>\n                            ))}\n                        </Card.Entities>\n\n                        <Card.Feature category={category}>\n                            <Player>\n                                <Player.Button />\n                                <Player.Video src=\"/videos/bunny.mp4\" />\n                            </Player>\n                        </Card.Feature>\n                    </Card>\n                ))}\n            </Card.Group>\n            <FooterContainer />\n        </>\n    ) : ( \n        <SelectProfileContainer user={user} setProfile={setProfile} />\n    );\n}",["189","190"],"/media/denis/New Volume/netflix/src/index.js",[],"/media/denis/New Volume/netflix/src/lib/firebase.prod.js",[],"/media/denis/New Volume/netflix/src/pages/index.js",[],"/media/denis/New Volume/netflix/src/pages/browse.js",[],"/media/denis/New Volume/netflix/src/pages/home.js",[],"/media/denis/New Volume/netflix/src/containers/footer.js",[],["191","192"],"/media/denis/New Volume/netflix/src/containers/jumbotron.js",[],"/media/denis/New Volume/netflix/src/containers/profiles.js",[],"/media/denis/New Volume/netflix/src/components/header/index.js",[],"/media/denis/New Volume/netflix/src/components/footer/index.js",[],"/media/denis/New Volume/netflix/src/components/form/index.js",[],"/media/denis/New Volume/netflix/src/components/header/styles/header.js",[],"/media/denis/New Volume/netflix/src/components/form/styles/form.js",[],"/media/denis/New Volume/netflix/src/app.js",[],["193","194"],"/media/denis/New Volume/netflix/src/context/firebase.js",[],"/media/denis/New Volume/netflix/src/pages/signin.js",[],"/media/denis/New Volume/netflix/src/containers/header.js",[],"/media/denis/New Volume/netflix/src/utils/selection-filter.js",[],"/media/denis/New Volume/netflix/src/hooks/use-auth-listener.js",["195"],"import { useState, useEffect, useContext } from \"react\";\nimport { FirebaseContext } from \"../context/firebase\";\n\nfunction useAuthListener() {\n    const [user, setUser] = useState(\n        JSON.parse(localStorage.getItem('authUser'))\n    );\n    \n    const { firebase } = useContext(FirebaseContext);\n\n    useEffect(() => {\n        const listener = firebase.auth().onAuthStateChanged((authUser) => {\n            if (authUser) {\n                localStorage.setItem('authUser', JSON.stringify(authUser));\n                setUser(authUser);\n            } else {\n                localStorage.removeItem('authUser');\n                setUser(null);\n            }\n        });\n\n        return() => listener();\n    }, []);\n\n    return { user };\n};\n\nexport default useAuthListener;","/media/denis/New Volume/netflix/src/components/opt-form/index.js",[],"/media/denis/New Volume/netflix/src/components/profiles/styles/profiles.js",[],"/media/denis/New Volume/netflix/src/components/loading/styles/loading.js",[],"/media/denis/New Volume/netflix/src/components/footer/styles/footer.js",[],"/media/denis/New Volume/netflix/src/helpers/routes.js",[],"/media/denis/New Volume/netflix/src/components/jumbotron/styles/jumbotron.js",[],"/media/denis/New Volume/netflix/src/components/accordion/index.js",[],"/media/denis/New Volume/netflix/src/components/feature/styles/feature.js",[],"/media/denis/New Volume/netflix/src/global-styles.js",[],"/media/denis/New Volume/netflix/src/pages/signup.js",[],"/media/denis/New Volume/netflix/src/hooks/index.js",[],"/media/denis/New Volume/netflix/src/hooks/use-content.js",["196"],"import { useEffect, useContext, useState } from 'react';\nimport { FirebaseContext } from '../context/firebase';\n\nexport default function useContent(target) {\n    const [content, setContent] = useState();\n    const {firebase} = useContext(FirebaseContext);\n\n    useEffect(() => {\n        firebase\n            .firestore()\n            .collection(target)\n            .get()\n            .then((snapshot) => {\n                const allContent = snapshot.docs.map((contentObj) => ({\n                    ...contentObj.data(),\n                    docId: contentObj.id,\n                }));\n\n                setContent(allContent);\n            })\n            .catch((error) => {\n                console.log(error.message);\n            });\n\n    }, []);\n\n    return { [target]: content };\n}","/media/denis/New Volume/netflix/src/components/index.js",[],"/media/denis/New Volume/netflix/src/components/jumbotron/index.js",[],"/media/denis/New Volume/netflix/src/components
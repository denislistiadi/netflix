n\nexport const PlayerContext = createContext();\n\nfunction Player({ children, ...restProps }) {\n    const [showPlayer, setShowPlayer] = useState();\n\n    return (\n        <PlayerContext.Provider value={{ showPlayer, setShowPlayer}}>\n            <Container {...restProps}>{children}</Container>\n        </PlayerContext.Provider>\n    )\n}\n\nPlayer.Video = function PlaterVideo({src, ...restProps}) {\n    const {showPlayer, setShowPlayer} = useContext(PlayerContext);\n\n    return showPlayer ? ReactDOM.createPortal(\n        <Overlay onClick={() => setShowPlayer(false)}>\n            <Inner>\n                <video id=\"netflix-player\" controls>\n                    <source src={src} type=\"video/mp4\" />\n                </video>\n            </Inner>\n        </Overlay>,\n\n        document.body\n    ) : null;\n}\n\nPlayer.Button = function PlayerButton({...restProps}) {\n    const {showPlayer, setShowPlayer} = useContext(PlayerContext);\n\n    return (\n        <Button onClick={() => setShowPlayer((showPlayer) => !showPlayer)}>\n            Play\n        </Button>\n    )\n}\n\nexport default Player\n","/media/denis/New Volume/netflix/src/components/player/styles/player.js",[],"/media/denis/New Volume/netflix/src/components/accordion/styles/accordion.js",[],"/media/denis/New Volume/netflix/src/components/loading/index.js",[],{"ruleId":"197","replacedBy":"198"},{"ruleId":"199","replacedBy":"200"},{"ruleId":"201","severity":1,"message":"202","line":43,"column":8,"nodeType":"203","endLine":43,"endColumn":20,"suggestions":"204"},{"ruleId":"197","replacedBy":"205"},{"ruleId":"199","replacedBy":"206"},{"ruleId":"197","replacedBy":"207"},{"ruleId":"199","replacedBy":"208"},{"ruleId":"197","replacedBy":"209"},{"ruleId":"199","replacedBy":"210"},{"ruleId":"201","severity":1,"message":"211","line":23,"column":8,"nodeType":"203","endLine":23,"endColumn":10,"suggestions":"212"},{"ruleId":"197","replacedBy":"213"},{"ruleId":"199","replacedBy":"214"},{"ruleId":"201","severi
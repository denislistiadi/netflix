', JSON.stringify(authUser));\n                setUser(authUser);\n            } else {\n                localStorage.removeItem('authUser');\n                setUser(null);\n            }\n        });\n\n        return() => listener();\n    }, []);\n\n    return { user };\n};\n\nexport default useAuthListener;","/media/denis/New Volume/netflix/src/components/opt-form/index.js",[],"/media/denis/New Volume/netflix/src/components/profiles/styles/profiles.js",[],"/media/denis/New Volume/netflix/src/components/loading/styles/loading.js",[],"/media/denis/New Volume/netflix/src/components/footer/styles/footer.js",[],"/media/denis/New Volume/netflix/src/helpers/routes.js",[],"/media/denis/New Volume/netflix/src/components/jumbotron/styles/jumbotron.js",[],"/media/denis/New Volume/netflix/src/components/accordion/index.js",[],"/media/denis/New Volume/netflix/src/components/feature/styles/feature.js",[],"/media/denis/New Volume/netflix/src/global-styles.js",[],"/media/denis/New Volume/netflix/src/pages/signup.js",[],"/media/denis/New Volume/netflix/src/hooks/index.js",[],"/media/denis/New Volume/netflix/src/hooks/use-content.js",["203"],"import { useEffect, useContext, useState } from 'react';\nimport { FirebaseContext } from '../context/firebase';\n\nexport default function useContent(target) {\n    const [content, setContent] = useState();\n    const {firebase} = useContext(FirebaseContext);\n\n    useEffect(() => {\n        firebase\n            .firestore()\n            .collection(target)\n            .get()\n            .then((snapshot) => {\n                const allContent = snapshot.docs.map((contentObj) => ({\n                    ...contentObj.data(),\n                    docId: contentObj.id,\n                }));\n\n                setContent(allContent);\n            })\n            .catch((error) => {\n                console.log(error.message);\n            });\n\n    }, []);\n\n    return { [target]: content };\n}","/media/denis/New Volume/netflix/src/compone